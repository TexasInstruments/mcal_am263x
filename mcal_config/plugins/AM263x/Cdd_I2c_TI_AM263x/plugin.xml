<plugin>
    <extension point="dreisoft.tresos.launcher2.plugin.module"
             id="Cdd_I2c_TI_AM263x_ModuleId"
             name="Cdd_I2c_TI_AM263x Module">

    <module id="Cdd_I2c_TI_AM263x"
            label="Cdd_I2c"
            mandatory="false"
            allowMultiple="false"
            description="TI Sitara MCU Cdd I2c Plugin"
            copyright="(c) 2022 Texas Instruments Incorporated"
            swVersionMajor="11"
            swVersionMinor="0"
            swVersionPatch="1"
            swVersionSuffix=""
            specVersionMajor="4"
            specVersionMinor="6"
            specVersionPatch="0"
            specVersionSuffix=""
            relVersionPrefix="AUTOSAR"
            relVersionMajor="4"
            relVersionMinor="3"
            relVersionPatch="1"
            relVersionSuffix="0"
            categoryType="Cdd_I2c"
            categoryLayer="Complex Device Driver"
            categoryCategory="CDD"
            categoryComponent="ECUC">
      <ecuType target="ARM" derivate="AM263x"/>
    </module>
  </extension>


    <extension point="dreisoft.tresos.launcher2.plugin.configuration"
             id="Cdd_I2c_TI_AM263x_ConfigId"
             name="Cdd_I2c_TI_AM263x Configuration">
    <configuration moduleId="Cdd_I2c_TI_AM263x">
      <schema>
        <manager class="dreisoft.tresos.autosar2.resourcehandling.AutosarSchemaManager"/>
        <resource value="config/Cdd_I2c.xdm" type="xdm" id="res_default"/>
		<resource value="config_ext/Cdd_I2c_Rec.xdm" type="xdm" id="res_recconf"/>
        <recconfig name="Default Recommended Configuration"
                   default="true"
                   description="Default Recommended Configuration"
                   resourceId="res_recconf"
                   path="ASPath:/TI_AM263x/Cdd_I2c_Rec"/>
      </schema>

      <data>
        <manager class="dreisoft.tresos.autosar2.resourcehandling.AutosarConfigManager"/>
        <schemaNode path="ASPath:/TI_AM263x/Cdd_I2c"/>
      </data>

      <editor id="Cdd_I2c_TI_AM263x_EditorId"
              label="Default"
              tooltip="TI AutoRadar CDD plug-in">
        <class class="dreisoft.tresos.launcher2.editor.GenericConfigEditor">
          <parameter name="schema" value="ASPath:/TI_AM263x/Cdd_I2c"/>
          <parameter name="title" value="Cdd_I2c"/>
          <parameter name="noTabs" value="false"/>
          <parameter name="noLinks" value="true"/>
          <parameter name="groupLinks" value="false"/>
          <parameter name="groupContainers" value="false"/>
          <parameter name="groupTables" value="true"/>
          <parameter name="optionalGeneralTab" value="true"/>
        </class>
      </editor>
    </configuration>
  </extension>
  <extension point="dreisoft.tresos.launcher2.plugin.generator"
              id="Cdd_I2c_TI_AM263x_GeneratorId"
              name="Cdd_I2c_TI_AM263x Generator">
    <generator
          class="dreisoft.tresos.launcher2.generator.TemplateBasedCodeGenerator"
          id="CddI2CGenerator_AM263x"
          moduleId="Cdd_I2c_TI_AM263x">

      <!-- common template path parameters -->
      <parameter name="templates"
                 mode="generate,verify" value="generate,generate_swcd"/>

    </generator>
  </extension>


  <extension point="org.eclipse.help.toc">
    <toc file="anchors.xml" primary="true"/>
  </extension>

</plugin>

