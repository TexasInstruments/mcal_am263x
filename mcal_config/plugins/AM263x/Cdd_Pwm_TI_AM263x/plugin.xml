<plugin>
    <extension point="dreisoft.tresos.launcher2.plugin.module"
             id="Cdd_Pwm_TI_AM263x_ModuleId"
             name="Cdd_Pwm_TI_AM263x Module">

    <module id="Cdd_Pwm_TI_AM263x"
            label="Cdd_Pwm"
            mandatory="false"
            allowMultiple="false"
            description="TI Sitara MCU AM263x Cdd_Pwm plug-in"
            copyright="(c) 2024 Texas Instruments Incorporated"
            swVersionMajor="10"
            swVersionMinor="2"
            swVersionPatch="0"
            swVersionSuffix=""
            specVersionMajor="4"
            specVersionMinor="3"
            specVersionPatch="1"
            specVersionSuffix=""
            relVersionPrefix="AUTOSAR"
            relVersionMajor="4"
            relVersionMinor="3"
            relVersionPatch="1"
            relVersionSuffix="0"
            categoryType="Cdd_Pwm"
            categoryLayer="Microcontroller Drivers"
            categoryCategory="PWM"
            categoryComponent="ECUC">
      <ecuType target="ARM" derivate="AM263x"/>
    </module>
  </extension>


    <extension point="dreisoft.tresos.launcher2.plugin.configuration"
             id="Cdd_Pwm_TI_AM263x_ConfigId"
             name="Cdd_Pwm_TI_AM263x Configuration">
    <configuration moduleId="Cdd_Pwm_TI_AM263x">
      <schema>
        <manager class="dreisoft.tresos.autosar2.resourcehandling.AutosarSchemaManager"/>
        <resource value="config/Cdd_Pwm.xdm" type="xdm" id="res_default"/>
                              
        <resource value="config_ext/Cdd_Pwm_Rec.xdm" type="xdm" id="res_recconf"/>
        
            <recconfig name="Default Recommended Configuration"
                   default="true"
                   description="Default Recommended Configuration"
                   resourceId="res_recconf"
                   path="ASPath:/TI_AM263x/Cdd_Pwm_Rec"/>
      </schema>

      <data>
        <manager class="dreisoft.tresos.autosar2.resourcehandling.AutosarConfigManager"/>
        <schemaNode path="ASPath:/TI_AM263x/Cdd_Pwm"/>
      </data>

      <editor id="Cdd_Pwm_TI_AM263x_EditorId"
              label="Default"
              tooltip="TI Sitara MCU Cdd_Pwm plug-in">
        <class class="dreisoft.tresos.launcher2.editor.GenericConfigEditor">
          <parameter name="schema" value="ASPath:/TI_AM263x/Cdd_Pwm"/>
          <parameter name="title" value="Cdd_Pwm"/>
          <parameter name="noTabs" value="false"/>
          <parameter name="noLinks" value="true"/>
          <parameter name="groupLinks" value="false"/>
          <parameter name="groupContainers" value="false"/>
          <parameter name="groupTables" value="true"/>
          <parameter name="optionalGeneralTab" value="true"/>
        </class>
      </editor>
    </configuration>
  </extension>
  <extension point="dreisoft.tresos.launcher2.plugin.generator"
              id="Cdd_Pwm_TI_AM263x_GeneratorId"
              name="Cdd_Pwm_TI_AM263x Generator">
    <generator 
               class="dreisoft.tresos.launcher2.generator.TemplateBasedCodeGenerator"
               id="CddPwmGenerator"
               moduleId="Cdd_Pwm_TI_AM263x">

      <!-- common template path parameters -->
      <parameter name="templates"
                 mode="generate,verify" value="generate,generate_swcd"/>
    </generator>
  </extension>

 
  <extension point="org.eclipse.help.toc">
    <toc file="anchors.xml" primary="true"/>
  </extension>
</plugin>

