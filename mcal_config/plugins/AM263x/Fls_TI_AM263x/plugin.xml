<plugin>
    <extension point="dreisoft.tresos.launcher2.plugin.module"
             id="Fls_TI_AM263x_ModuleId"
             name="Fls_TI_AM263x Module">

    <module id="Fls_TI_AM263x"
            label="Fls"
            mandatory="false"
            allowMultiple="false"
            description="TI Sitara MCU Fls plug-in"
            copyright="(c) 2022 Texas Instruments Incorporated"
            swVersionMajor="10"
            swVersionMinor="2"
            swVersionPatch="0"
            swVersionSuffix=""
            specVersionMajor="4"
            specVersionMinor="3"
            specVersionPatch="1"
            specVersionSuffix=""
            relVersionPrefix="AUTOSAR"
            relVersionMajor="4"
            relVersionMinor="3"
            relVersionPatch="1"
            relVersionSuffix="0"
            categoryType="Fls"
            categoryLayer="Memory Drivers"
            categoryCategory="FLS"
            categoryComponent="ECUC">
      <ecuType target="ARM" derivate="AM263x"/>
    </module>
  </extension>


    <extension point="dreisoft.tresos.launcher2.plugin.configuration"
             id="Fls_TI_AM263x_ConfigId"
             name="Fls_TI_AM263x Configuration">
    <configuration moduleId="Fls_TI_AM263x">
      <schema>
        <manager class="dreisoft.tresos.autosar2.resourcehandling.AutosarSchemaManager"/>
        <resource value="config/Fls.xdm" type="xdm" id="res_default"/>
        <resource value="config_ext/Fls_Rec.xdm" type="xdm" id="res_recconf"/>
        <recconfig name="Default Recommended Configuration"
                   default="true"
                   description="Default Recommended Configuration"
                   resourceId="res_recconf"
                   path="ASPath:/TI_AM263x/Fls_Rec"/>
      </schema>

      <data>
        <manager class="dreisoft.tresos.autosar2.resourcehandling.AutosarConfigManager"/>
        <schemaNode path="ASPath:/TI_AM263x/Fls"/>
      </data>

      <editor id="Fls_TI_AM263x_EditorId"
              label="Default"
              tooltip="TI AutoRadar Fls plug-in">
        <class class="dreisoft.tresos.launcher2.editor.GenericConfigEditor">
          <parameter name="schema" value="ASPath:/TI_AM263x/Fls"/>
          <parameter name="title" value="Fls"/>
          <parameter name="noTabs" value="false"/>
          <parameter name="noLinks" value="true"/>
          <parameter name="groupLinks" value="false"/>
          <parameter name="groupContainers" value="false"/>
          <parameter name="groupTables" value="true"/>
          <parameter name="optionalGeneralTab" value="true"/>
        </class>
      </editor>
    </configuration>
  </extension>
  <extension point="dreisoft.tresos.launcher2.plugin.generator"
              id="Fls_TI_AM263x_GeneratorId"
              name="Fls_TI_AM263x Generator">
    <generator
          class="dreisoft.tresos.launcher2.generator.TemplateBasedCodeGenerator"
          id="FlsGenerator_AM263x"
          moduleId="Fls_TI_AM263x">

      <!-- common template path parameters -->
      <parameter name="templates"
                 mode="generate,verify" value="generate,generate_swcd"/>

    </generator>
  </extension>
  <!-- definition of ecu resource properties files -->
  <extension
        id="Fls_TI_AM263x_EcuResourceId"
        name="Fls_TI_AM263x EcuResource"
        point="dreisoft.tresos.autosar2.api.plugin.ecuresource">
     <ecuresource
           derivate="AM263x"
           file="resources/AM263x_Fls.properties"
           moduleId="Fls_TI_AM263x"
           target="ARM">
     </ecuresource>
  </extension>
  <extension
         point="dreisoft.tresos.autosar2.api.plugin.ecuresourcefinder">
      <ecuresourcefinder>
         <finder
            class="dreisoft.tresos.autosar2.api.ecuresource.DefaultEcuResourceFinder">
         </finder>
      </ecuresourcefinder>
  </extension>
  <extension point="org.eclipse.help.toc">
    <toc file="anchors.xml" primary="true"/>
  </extension>

</plugin>
