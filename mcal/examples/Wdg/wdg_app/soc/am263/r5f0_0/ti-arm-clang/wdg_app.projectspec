<projectSpec>
    <applicability>
        <when>
            <context
                deviceFamily="ARM"
                deviceId="Cortex R.AM263x"
            />
        </when>
    </applicability>

    <project
        title="wdg_app"
        name = "wdg_app"
        configurations="
            Release,
            Debug,
        "
        connection="TIXDS110_Connection.xml"
        toolChain="TICLANG"
        cgtVersion="4.0.3.LTS"
        device="Cortex R.AM263x"
        deviceCore="Cortex_R5_0"
        ignoreDefaultDeviceSettings="true"
        ignoreDefaultCCSSettings="true"
        endianness="little"
        outputType="executable"
        outputFormat="ELF"
        launchWizard="False"
        linkerCommandFile=""
        compilerBuildOptions_asm="
        -x ti-asm
        -mlittle-endian
        -mcpu=cortex-r5
        -mfloat-abi=hard
        -mfpu=vfpv3-d16
        -Wno-unused-command-line-argument
        -mthumb
        -save-temps
        -c
        "
        artifactExtension="xer5f"
        compilerBuildOptions_c="
        -mlittle-endian
        -mcpu=cortex-r5
        -gdwarf-3
        -mfloat-abi=hard
        -mfpu=vfpv3-d16
        -mthumb
        -Wall
        -Werror
        -Wno-gnu-variable-sized-type-not-at-end
        -Wno-unused-function
        -DAM263X_PLATFORM
        -ffunction-sections
        -DCLANG
        -DTRACE_ENABLE
        -DRELEASE_BUILD
        -DASSERT_ENABLE
        -DAUTOSAR_431
        -I${MCAL_MCAL_autosar_include}
        -I${MCAL_MCAL_Wdg}
        -I${MCAL_MCAL_Include_hw}
        -I${MCAL_MCAL_Mcal_Lib}
        -I${MCAL_MCAL_Include_memmap}
        -I${MCAL_MCAL_Include_hw_types}
        -I${MCAL_MCAL_Include_hal_stdtypes}
        -I${MCAL_MCAL_Mcu}
        -I${MCAL_MCAL_Port}
        -I${MCAL_MCAL_Gpt}

        -I${MCAL_MCAL_Wdg_eg}
        -I${MCAL_MCAL_Utils_am263}
        -I${MCAL_MCAL_Utils}
        -I${MCAL_MCAL_Wdg_Cfg}
        -I${MCAL_MCAL_Mcu_Cfg}
        -I${MCAL_MCAL_Port_Cfg}
        -I${MCAL_MCAL_Gpt_Cfg}


        -I${MCAL_MCAL_Gpt_V0}
        -I${MCAL_MCAL_Wdg_V0}
        -I${MCAL_MCAL_Mcu_V0}
        -I${MCAL_MCAL_Port_V0}
        "
        postBuildStep="$(MAKE) -C ${CCS_PROJECT_DIR} -f makefile_ccs_bootimage_gen OUTNAME=wdg_app PROFILE=${ConfigName} MCAL_SITARA_MCU_PATH=${MCAL_MCAL}/.. CG_TOOL_ROOT=${CG_TOOL_ROOT} CCS_INSTALL_DIR=${CCS_INSTALL_DIR} CCS_NODE=${CCS_INSTALL_DIR}/tools/node/node CYGWIN_PATH=${CCS_UTILS_DIR}/cygwin DEVICE=am263x"

        >
        <configuration name="Debug"
            compilerBuildOptions_c="
                -D_DEBUG_=1
            "
            linkerBuildOptions="
            --diag_suppress=10068
            --diag_suppress=10063
            --ram_model
            --reread_libs
            "
        ></configuration>
        <configuration name="Release"
            compilerBuildOptions_c="
                -Os
            "
            linkerBuildOptions="
            --diag_suppress=10068
            --diag_suppress=10063
            --ram_model
            --reread_libs
            "
        ></configuration>

        <pathVariable name="MCAL_MCAL" path="../../../../../../../../mcal" scope="project"/>
        <pathVariable name="MCAL_MCAL_autosar_include" path="${MCAL_MCAL}/autosar_include" scope="project"/>
        <pathVariable name="MCAL_MCAL_Wdg" path="${MCAL_MCAL}/Wdg/include" scope="project"/>
        <pathVariable name="MCAL_MCAL_Mcu" path="${MCAL_MCAL}/Mcu/include" scope="project"/>
        <pathVariable name="MCAL_MCAL_Port" path="${MCAL_MCAL}/Port/include" scope="project"/>
        <pathVariable name="MCAL_MCAL_Gpt" path="${MCAL_MCAL}/Gpt/include" scope="project"/>
        <pathVariable name="MCAL_MCAL_makefile" path="${MCAL_MCAL}/examples/Wdg/wdg_app/soc/am263/r5f0_0/ti-arm-clang" scope="project"/>

        <pathVariable name="MCAL_MCAL_Include_hw" path="${MCAL_MCAL}/include/hw/am263" scope="project"/>
        <pathVariable name="MCAL_MCAL_Include_memmap" path="${MCAL_MCAL}/include/memmap" scope="project"/>
        <pathVariable name="MCAL_MCAL_Include_hw_types" path="${MCAL_MCAL}/include/hw/" scope="project"/>
        <pathVariable name="MCAL_MCAL_Include_hal_stdtypes" path="${MCAL_MCAL}/include/hw/hal_stdtypes.h" scope="project"/>
        <pathVariable name="MCAL_MCAL_Utils_am263" path="${PROJECT_LOC}/Utils/am263" scope="project"/>
